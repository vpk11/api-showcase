<% content_for :head do %>
  <%= stylesheet_pack_tag 'sessions', media: 'all', 'data-turbolinks-track': 'reload' %>
<% end %>
<% if @error %>
  <%= react_component('Login', {alertType: 'error', showAlert: true, alertMessage: @error}) %>
<% else %>
  <%= react_component('Login', {showAlert: false}) %>
<% end %>
